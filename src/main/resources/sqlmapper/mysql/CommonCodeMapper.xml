<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Home">
    <!-- call CommonCode.selectFromCIP_COMMON_CODEByCOMMON_CODE_ID -->
    <select id="selectFromCIP_COMMON_CODEByCOMMON_CODE_ID" parameterType="Map" resultType="Map">
        SELECT COMMON_CODE_ID, NAME, ORDER_NUMBER
        FROM cip_common_code
        WHERE COMMON_CODE_ID = #{COMMON_CODE_ID}
    </select>
</mapper>